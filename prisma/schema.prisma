generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Quiz {
  id        String   @id @default(cuid())
  title     String
  content   Json?
  live      Boolean  @default(false)
  createdAt DateTime @default(now())
}


